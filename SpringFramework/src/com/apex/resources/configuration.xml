<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id = "stud1" class = "com.apex.beans.Student">
    <property name="rollNo" value ="1"></property>
    <property name="name" value = "Shivanshu"></property>
    <property name="grade" value = "A-"></property>
    </bean>
    
    <bean id = "stud2" class = "com.apex.beans.Student">
    <property name="rollNo" value ="2"></property>
    <property name="name" value = "Abhishek"></property>
    <property name="grade" value = "A+"></property>
    </bean>

</beans>